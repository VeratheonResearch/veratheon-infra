ALPHA_VANTAGE_API_KEY=
OPENAI_API_KEY=
MODEL_SELECTED=gpt-4o-mini
LOCAL_OLLAMA_URL=http://localhost:11434
NORD_OLLAMA_URL=

# Supabase Configuration (Backend)
# Docker Compose overrides SUPABASE_URL with: http://host.docker.internal:54321
# This allows backend container to reach Supabase running on host machine
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_KEY=

# Supabase Configuration (Frontend - exposed to browser)
# Browser connects directly to Supabase on localhost (not through Docker)
# Production (Vercel): Use your hosted Supabase project URL
VITE_SUPABASE_URL=http://127.0.0.1:54321
VITE_SUPABASE_ANON_KEY=

# Backend API URL for frontend
# Docker Compose overrides this with: http://veratheon-research:8085
# Production (Vercel): https://veratheon-research.fly.dev
VITE_API_URL=http://localhost:8085
